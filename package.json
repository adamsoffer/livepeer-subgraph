{
  "name": "livepeer",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create adamsoffer/livepeer-canary --node https://api.thegraph.com/deploy/",
    "create-local": "graph create adamsoffer/livepeer-canary --node http://127.0.0.1:8020",
    "codegen": "graph codegen --debug --output-dir src/types/",
    "build": "graph build --debug",
    "deploy": "graph deploy adamsoffer/livepeer-canary --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy adamsoffer/livepeer-canary --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.7.1",
    "@graphprotocol/graph-ts": "^0.5.0"
  }
}
